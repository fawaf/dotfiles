alias cdd="cd ~/Desktop"
alias lao="ls -l --all -O"
alias lea="ls -l -e --all"
alias le="ls -l -e"
alias le@="ls -l -e --all -@"
alias llo="ls -l -O"
alias l@="ls -l -@"
alias ls="gls --group-directories-first --color=auto"
alias mv="mv -i -v"
alias rm="rm -i -v"
alias t="top -o -cpu -s 1"
alias ua="uname -a"
alias unlink="unlink -iv"
