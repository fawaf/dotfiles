alias ags="sudo systemctl reload httpd"
alias am="alsamixer"
alias arl="ags"
alias ars="ags"
alias asc="$SUDO httpd -S"
alias chl="tail --follow --retry /var/log/chef/client.log"
alias chmod="chmod --verbose"
alias cp="cp --interactive --verbose"
alias i6l="$SUDO ip6tables --list"
alias iln="$SUDO iptables --list --table nat --numeric"
alias ilnv="$SUDO iptables --list --table nat --numeric --verbose"
alias il="$SUDO iptables --list --numeric"
alias ilvn="$SUDO iptables --list --verbose --numeric"
alias ilv="$SUDO iptables --list --verbose"
alias ks="kill -SIGUSR1"
alias maims="maim --select"
alias maimsc="maims | xclip -target image/png"
alias nrl="sudo systemctl reload nginx"
alias nrs="sudo systemctl restart nginx"
alias nss="sudo systemctl start nginx"
alias nrt="sudo nginx -t"
alias ns="netstat -tln"
alias nsp="$SUDO netstat --tcp --listening --numeric --program"
alias nspu="$SUDO netstat --tcp --listening --numeric --program --udp"
alias psf="ps afjwwwwwx"
alias psw="ps afuwwwwwx"
alias rsync="rsync --iconv=utf8,utf8 --archive --verbose --compress --hard-links --progress --timeout=5 --rsh=ssh"
alias scrots="scrot --select --exec 'mv \$f /tmp/'"
alias t="top -o +%CPU -d 1"
alias ud="update"
alias us="$SUDO ufw status"
alias usn="$SUDO ufw status numbered"
alias www="cd /var/www"
