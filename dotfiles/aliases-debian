alias aa="tail --follow --retry /var/log/apache2/access.log"
alias ae="tail --follow --retry /var/log/apache2/error.log"
alias ags="sudo systemctl reload apache2"
alias arl="ags"
alias ars="ags"
alias asc="$SUDO sudo apache2ctl -S"
alias brl="sudo systemctl reload bind9"
alias brs="sudo systemctl restart bind9"
alias cmail="cat /var/log/mail.log"
alias cs="cat /var/log/syslog"
alias drs="sudo systemctl restart isc-dhcp-server"
alias faa="tail --follow --retry /var/log/apache2/fawong.com/access.log"
alias fae="tail --follow --retry /var/log/apache2/fawong.com/error.log"
alias fsa="tail --follow --retry /var/log/apache2/fawong.com/ssl_access.log"
alias fse="tail --follow --retry /var/log/apache2/fawong.com/ssl_error.log"
alias kaa="tail --follow --retry /var/log/apache2/kirinas.com/access.log"
alias kae="tail --follow --retry /var/log/apache2/kirinas.com/error.log"
alias ksa="tail --follow --retry /var/log/apache2/kirinas.com/ssl_access.log"
alias kse="tail --follow --retry /var/log/apache2/kirinas.com/ssl_error.log"
alias prl="sudo systemctl reload postfix"
alias prs="sudo systemctl restart postfix"
alias sa="tail --follow --retry /var/log/apache2/ssl_access.log"
alias se="tail --follow --retry /var/log/apache2/ssl_error.log"
alias td="tail --follow --retry /var/log/dhcp.log"
alias tmail="tail --follow --retry /var/log/mail.log"
alias tm="top -o +%MEM -d 1"
alias tna="tail --follow --retry /var/log/named/*.log"
alias tn="tail --follow --retry /var/log/named/named.log"
alias tq="tail --follow --retry /var/log/named/query.log"
alias ts="tail --follow --retry /var/log/syslog"
alias update="$SUDO apt update && $SUDO apt full-upgrade --assume-yes"
alias www="cd /var/www"
alias xaa="tail --follow --retry /var/log/apache2/xilef.org/access.log"
alias xae="tail --follow --retry /var/log/apache2/xilef.org/error.log"
alias xsa="tail --follow --retry /var/log/apache2/xilef.org/ssl_access.log"
alias xse="tail --follow --retry /var/log/apache2/xilef.org/ssl_error.log"
