. $HOME/.functions

# git {{{
alias ga.="git aa"
alias ga="git a"
alias gaa="git aa"
alias gb="git b"
alias gba="while read l;do cd \$l;git branch|grep --invert-match main;done"
alias gbam="while read l;do cd \$l;git branch|grep --invert-match master;done"
alias gbd="gco main; gl; gf --prune; git remote prune origin; git branch --delete"
alias gbdm="gco master; gl; gf --prune; git remote prune origin; git branch --delete"
alias gbm="gbsum"
alias gbsu="git bsu"
alias gbsum="git bsum"
alias gbsums="git bsums"
alias gc="git c"
alias gca="git ca"
alias gcaa="git caa"
alias gcd="git clean -d --force"
alias gcf="git clean --force"
alias gco.="git checkout -- ."
alias gco="git checkout"
alias gcp="git cherry-pick"
alias gcpc="git cherry-pick --continue"
alias gd="git diff"
alias gdi="git di"
alias gds="git ds"
alias gerp="grep"
alias gf="git f"
alias gfrm="git f && git rbms && git p; git pf"
alias gi="git info"
alias gitall="ls -d */ | xargs -P10 -I{} git -C {} "
alias gl="git pull"
alias glg="git log"
alias glo.="git log ."
alias glo="git log"
alias glop.="git log --patch ."
alias glop="git log --patch"
alias glot="git log --tags"
alias glu="git pull upstream"
alias glum="git pull upstream main"
alias glums="git pull upstream master"
alias glup="glum && gp"
alias glups="glums && gp"
alias gp="git p"
alias gpf="git pf"
alias gpff="git push --force"
alias gr="git restore"
alias gra="git remote add"
alias grau="git remote add upstream"
alias grb="git rebase --interactive"
alias grbm="git rebase --interactive origin/main"
alias grbms="git rbms"
alias grbs="git rebase --skip"
alias grc="git rebase --continue"
alias grep="grep --line-number --color"
alias grp="groups"
alias grpe="grep"
alias grs="git restore --staged"
alias gst.="git status ."
alias gst="git status"
alias rgh="rm --recursive --force --verbose .git/hooks && git init"
# }}}

# common {{{
alias a="alias"
