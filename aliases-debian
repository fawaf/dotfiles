alias ksa="tail --follow /var/log/apache2/kirinas.com/ssl_access.log"
alias kse="tail --follow /var/log/apache2/kirinas.com/ssl_error.log"
alias sa="tail --follow /var/log/apache2/ssl_access.log"
alias se="tail --follow /var/log/apache2/ssl_error.log"
alias sudo="sudo --preserve-env"
alias ts="tail --follow /var/log/syslog"
alias t="top -o +%CPU -d 1"
alias tm="top -o +%MEM -d 1"
alias tmail="tail --follow /var/log/mail.log"
alias update="echo y | sudo apt update && sudo apt full-upgrade"
alias www="cd /var/www"
