alias aa="tail -f /var/log/apache2/access.log"
alias ae="tail -f /var/log/apache2/error.log"
alias arl="sudo service apache2 reload"
alias ars="sudo service apache2 restart"
alias asc="sudo apache2ctl -S"
alias brl="sudo service bind9 reload"
alias brs="sudo service bind9 restart"
alias faa="tail -f /var/log/apache2/fawong/access.log"
alias fae="tail -f /var/log/apache2/fawong/error.log"
alias fsa="tail -f /var/log/apache2/fawong/ssl_access.log"
alias fse="tail -f /var/log/apache2/fawong/ssl_error.log"
alias sa="tail -f /var/log/apache2/ssl_access.log"
alias se="tail -f /var/log/apache2/ssl_error.log"
alias update="echo y | sudo aptitude update && echo y | sudo aptitude -y full-upgrade"
alias www="cd /var/www"
alias xaa="tail -f /var/log/apache2/xilef/access.log"
alias xae="tail -f /var/log/apache2/xilef/error.log"
alias xsa="tail -f /var/log/apache2/xilef/ssl_access.log"
alias xse="tail -f /var/log/apache2/xilef/ssl_error.log"
