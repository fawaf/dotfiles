alias aa="tail --follow /var/log/apache2/access.log"
alias ack="ack-grep"
alias ae="tail --follow /var/log/apache2/error.log"
alias ags="sudo service apache2 graceful"
alias arl="sudo service apache2 reload"
alias ars="sudo service apache2 restart"
alias asc="sudo apache2ctl -S"
alias brl="sudo service bind9 reload"
alias brs="sudo service bind9 restart"
alias cmail="cat /var/log/mail.log"
alias cs="cat /var/log/syslog"
alias faa="tail --follow /var/log/apache2/fawong.com/access.log"
alias fae="tail --follow /var/log/apache2/fawong.com/error.log"
alias fsa="tail --follow /var/log/apache2/fawong.com/ssl_access.log"
alias fse="tail --follow /var/log/apache2/fawong.com/ssl_error.log"
