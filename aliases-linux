alias ags="sudo service httpd graceful"
alias arl="sudo service httpd reload"
alias ars="sudo service httpd restart"
alias asc="sudo httpd -S"
alias chl="tail --follow /var/log/chef/client.log"
alias chmod="chmod --verbose"
alias i6l="sudo ip6tables --list"
alias il="sudo iptables --list"
alias ls="ls --group-directories-first --color=auto"
alias ns="netstat -tln"
alias nsp="sudo netstat -tlnp"
alias psf="ps axjfwww"
alias ud="update"
alias us="sudo ufw status"
alias www="cd /var/www"
