alias aa="tail --follow --retry /var/log/apache2/access.log"
alias ae="tail --follow --retry /var/log/apache2/error.log"
alias ags="sudo service apache2 graceful"
alias arl="sudo service apache2 reload"
alias ars="ags"
alias asc="sudo apache2ctl -S"
alias brl="sudo service bind9 reload"
alias brs="sudo service bind9 restart"
alias faa="tail --follow --retry /var/log/apache2/fawong.com/access.log"
alias fae="tail --follow --retry /var/log/apache2/fawong.com/error.log"
alias fsa="tail --follow --retry /var/log/apache2/fawong.com/ssl_access.log"
alias fse="tail --follow --retry /var/log/apache2/fawong.com/ssl_error.log"
alias kaa="tail --follow --retry /var/log/apache2/kirinas.com/access.log"
alias kae="tail --follow --retry /var/log/apache2/kirinas.com/error.log"
alias ksa="tail --follow --retry /var/log/apache2/kirinas.com/ssl_access.log"
alias kse="tail --follow --retry /var/log/apache2/kirinas.com/ssl_error.log"
alias sa="tail --follow --retry /var/log/apache2/ssl_access.log"
alias se="tail --follow --retry /var/log/apache2/ssl_error.log"
alias t="top -o +%CPU -d 1"
alias update="echo y | sudo zypper refresh && sudo zypper upgrade"
alias www="cd /var/www"
